pipeline {
    agent any
    stages {
        stage('Rezando') {
            steps {
                echo 'Rezando a los dioses de DevOps...'
            }
        }
        stage('Alivio') {
            steps {
                echo 'Comprobar que el webhook llega'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
